      <projectItem id="1081" key="673e7d60-86f2-4a00-9c16-94725c466d31" parentID="1079" level="3" creatorID="0" sortOrder="1" createDate="2018-03-06T16:55:47" updateDate="2018-03-24T17:26:14" nodeName="Multi-Page Responsive Website" urlName="multi-page-responsive-website" path="-1,1076,1079,1081" isDoc="" nodeType="1074" creatorName="Gintas Pociunas" writerName="Gintas Pociunas" writerID="0" template="1073" nodeTypeAlias="projectItem" isPublished="true">
