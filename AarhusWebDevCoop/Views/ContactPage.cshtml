@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.ContactPage>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
    Layout = "Master.cshtml";
}
<div class="writing-video">
    <video id="video" preload="none" autoplay loop>
        <source id="mp4" src="~/Media/writing.mp4" type="video/mp4">
    </video>
</div>
<div class="fullsection">
    <br />
    <br />
    <br />
    <section>
        <h1 class="light text-center">@CurrentPage.Name</h1>
        <hr class="col-sm-6 light" />
    </section>

    <section>
        <div class="col-lg-12 text-center">
            @Html.Action("Index", "ContactFormSurface")
        </div>
    </section>
</div>